<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="425.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PhotoInjector">
   <children>
      <ImageView fx:id="imagepath" fitHeight="150.0" fitWidth="200.0" layoutX="44.0" layoutY="33.0" pickOnBounds="true" />
      <Text layoutX="47.0" layoutY="254.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Width" />
      <TextField fx:id="width" layoutX="93.0" layoutY="237.0" prefHeight="25.0" prefWidth="41.0" text="8" />
      <Text layoutX="152.0" layoutY="254.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Height" />
      <TextField fx:id="height" layoutX="203.0" layoutY="237.0" prefHeight="25.0" prefWidth="41.0" text="8" />
      <CheckBox fx:id="activate_cbx" layoutX="152.0" layoutY="287.0" mnemonicParsing="false" text="Activate" />
      <CheckBox fx:id="setcoords_cbx" layoutX="47.0" layoutY="287.0" mnemonicParsing="false" text="Set Coords" />
      <ProgressBar fx:id="bar" layoutX="47.0" layoutY="200.0" prefWidth="200.0" progress="0.0" />
      <CheckBox fx:id="always_on_top_cbx" layoutX="292.0" layoutY="358.0" mnemonicParsing="false" onAction="#toggleAlwaysOnTop" text="Always On Top" />
      <Button fx:id="buttoninject" layoutX="292.0" layoutY="250.0" mnemonicParsing="false" onAction="#injectphoto" prefHeight="41.0" prefWidth="112.0" text="Inject" />
      <Button layoutX="292.0" layoutY="198.0" mnemonicParsing="false" onAction="#importphoto" prefHeight="41.0" prefWidth="112.0" text="Import" />
      <RadioButton fx:id="leftdirection" layoutX="47.0" layoutY="386.0" mnemonicParsing="false" selected="true" text="Left">
         <toggleGroup>
            <ToggleGroup fx:id="directions" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rightdirection" layoutX="124.0" layoutY="386.0" mnemonicParsing="false" text="Right" toggleGroup="$directions" />
      <RadioButton fx:id="posterfurni" layoutX="47.0" layoutY="343.0" mnemonicParsing="false" selected="true" text="Poster">
         <toggleGroup>
            <ToggleGroup fx:id="furnitype" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="photofurni" layoutX="124.0" layoutY="343.0" mnemonicParsing="false" text="Photo" toggleGroup="$furnitype" />
      <Text layoutX="292.0" layoutY="327.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Delay" />
      <TextField fx:id="delay" layoutX="339.0" layoutY="310.0" prefHeight="25.0" prefWidth="64.0" text="4000" />
   </children>
</AnchorPane>
